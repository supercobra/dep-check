{
  "name": "q-desktop-v2",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve --proxy-config proxy.conf.js\" \"npm run server\" \"tail -F ~/.quio2/quio.log\" ",
    "server": "concurrently \"cd src/electron-dev/ && node api-dev.js\"  \"yarn electron-dev\"",
    "electron-dev": "yarn build-electron-dev && electron dist",
    "start-prod": "yarn electron",
    "dist": "electron dist",
    "build": "ng build",
    "test": "ng lint & ng test",
    "lint": "ng lint",
    "component": "ng generate component",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "build-electron": "ng build --base-href ./ && cp src/electron/* dist",
    "build-electron-dev": "ng build --base-href ./ && cp src/electron-dev/* dist",
    "run-electron": "cd src/electron-dev && electron .",
    "electron": "yarn build-electron && electron dist",
    "executable-linux": "npm run start && cd dist && npm install && cd .. && npm run package-linux && npm run asar-linux",
    "executable-win": "npm run start && cd dist && npm install && cd .. && npm run package-win && npm run asar-win",
    "executable-mac": "cd dist && npm install && cd .. && npm run package-mac ",
    "package-mac": "electron-packager ./dist q-Desktop --overwrite --platform=darwin --arch=x64  --icon=icon.icns",
    "package-win": "electron-packager ./dist q-Desktop --overwrite --platform=win32  --arch=ia32 --icon=icon.icns",
    "package-linux": "electron-packager ./dist q-Desktop --overwrite --platform=linux  --arch=x64  --icon=icon.icns",
    "asar-linux": "asar pack q-Desktop-linux-x64/resources/app q-Desktop-linux-x64/resources/app.asar && cd q-Desktop-linux-x64/resources && rm -rf app && cd ..",
    "asar-win": "asar pack q-Desktop-win32-ia32/resources/app q-Desktop-win32-ia32/resources/app.asar && cd q-Desktop-win32-ia32/resources && rm -rf app && cd .."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.3.1",
    "@angular/common": "^4.3.1",
    "@angular/compiler": "^4.3.1",
    "@angular/core": "^4.3.1",
    "@angular/forms": "^4.3.1",
    "@angular/http": "^4.3.1",
    "@angular/platform-browser": "^4.3.1",
    "@angular/platform-browser-dynamic": "^4.3.1",
    "@angular/router": "^4.3.1",
    "@ngx-translate/http-loader": "^1.0.2",
    "body-parser": "^1.17.2",
    "bootstrap": "^3.3.7",
    "brfs": "^1.4.3",
    "browserify-fs": "^1.0.0",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "electron-prebuilt": "^1.4.13",
    "electron-reload": "^1.2.1",
    "express": "^4.15.4",
    "express-basic-auth": "^1.1.2",
    "fs": "0.0.1-security",
    "fs.extra": "^1.3.2",
    "jquery": "^3.2.1",
    "json-loader": "^0.5.7",
    "jwt-simple": "^0.5.1",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "node": "^8.3.0",
    "node-notifier": "^5.1.2",
    "opn": "^5.1.0",
    "os-locale": "^2.1.0",
    "randomstring": "^1.1.5",
    "rxjs": "^5.1.0",
    "server-destroy": "^1.0.1",
    "winston": "^2.3.1",
    "ws": "^3.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.2.1",
    "@angular/compiler-cli": "^4.3.1",
    "@angular/language-service": "^4.3.1",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.85",
    "brfs": "^1.4.3",
    "codelyzer": "~3.0.1",
    "concurrently": "^3.5.0",
    "electron": "^1.7.6",
    "electron-packager": "^9.0.1",
    "font-awesome": "^4.7.0",
    "jasmine": "^2.7.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.1.2",
    "raw-loader": "^0.5.1",
    "ts-node": "^3.3.0",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
}
